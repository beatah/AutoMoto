@using AutoMoto.Contracts
@model IEnumerable<AutoMoto.Model.Models.Feature>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Wyposażenie</h1>
    </div>

</div>

<div class="row">
    <div class="col-lg-6">
        <a href="@Url.Action("Create","Feature")" class="btn btn-primary">
            <i class="fa fa-plus"></i> Dodaj
        </a>
    </div>
    <div class="col-lg-8">
        <div class="table-responsive">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Nazwa</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var feature in Model)
                    {
                        <tr>
                            <td>@feature.Name</td>
                            
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

